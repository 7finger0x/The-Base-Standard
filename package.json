{
  "name": "baserank-protocol",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "test:all": "npm run test:frontend && npm run test:indexer && npm run test:agent && npm run foundry:test",
    "test:frontend": "vitest run",
    "test:indexer": "cd apps/indexer && vitest run",
    "test:agent": "cd apps/agent && pytest",
    "test:agent:coverage": "cd apps/agent && pytest --cov",
    "dev:indexer": "cd apps/indexer && pnpm dev",
    "dev:agent": "cd apps/agent && python main.py",
    "foundry:build": "cd foundry && forge build",
    "foundry:test": "cd foundry && forge test -vvv",
    "foundry:deploy": "cd foundry && forge script script/Deploy.s.sol --broadcast --verify",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:push": "prisma db push",
    "db:pull": "prisma db pull"
  },
  "dependencies": {
    "@coinbase/onchainkit": "^0.36.11",
    "@farcaster/frame-sdk": "^0.1.0",
    "@neondatabase/serverless": "^1.0.2",
    "@prisma/adapter-neon": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "@tanstack/react-query": "^5.90.16",
    "@vercel/speed-insights": "^1.3.1",
    "clsx": "^2.1.0",
    "next": "^15.0.0",
    "prisma": "^7.2.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^2.5.0",
    "viem": "^2.44.1",
    "wagmi": "^2.19.5",
    "ws": "^8.19.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^16.0.0",
    "@types/node": "^22.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/ui": "^3.0.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^9.0.0",
    "eslint-config-next": "^15.0.0",
    "jsdom": "^23.0.1",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.6.0",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
