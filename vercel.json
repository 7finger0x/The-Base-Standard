{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_BASE_RPC_URL": "@base-rpc-url",
    "NEXT_PUBLIC_REGISTRY_ADDRESS": "@registry-address",
    "NEXT_PUBLIC_ONCHAINKIT_API_KEY": "@onchainkit-api-key",
    "PONDER_URL": "@ponder-url"
  }
}
